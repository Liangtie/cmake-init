{
  "name": "{= name =}",
  "version-semver": "{= version =}",
  "dependencies": [
    {
      "name": "{% if c %}json-c{% else %}fmt{% end %}",
      "version>=": "{% if c %}2022-06-26#3{% else %}10.1.0{% end %}"
    }{% if c and exe %},
    {
      "name": "hedley",
      "version>=": "15"
    }{% end %}
  ],
  "default-features": [],
  "features": {
    "test": {
      "description": "Dependencies for testing",
      "dependencies": [
        {% if catch3 %}{
          "name": "catch2",
          "version>=": "3.4.0"
        }{% else %}"catch2"{% end %}
      ]
    }
  },{% if not catch3 %}
  "overrides": [
    { "name": "catch2", "version": "2.13.9#1" }
  ],{% end %}
  "builtin-baseline": "962e5e39f8a25f42522f51fffc574e05a3efd26b"
}
